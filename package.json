{
  "name": "jacobparis.com",
  "main": "server.ts",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:info": "node -r esbuild-register -r dotenv/config others/build-info.js",
    "build:redirects": "cp ./app/_redirects ./build/_redirects",
    "predev": "npm run setup",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:content": "node -r esbuild-register -r dotenv/config others/refresh-on-content-change.ts",
    "dev:remix": "remix watch",
    "dev:node": "NODE_ENV=development tsx --require dotenv/config --require ./mocks ./server.ts --watch ./server.ts",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy",
    "start": "NODE_ENV=production tsx ./server.ts",
    "start:mocks": "node --inspect --require ./node_modules/dotenv/config --require ./mocks ./build/index.js",
    "typecheck": "tsc -b ",
    "validate": "run-p lint typecheck",
    "validate:deployable": "node -r esbuild-register -r dotenv/config others/is-deployable.js"
  },
  "dependencies": {
